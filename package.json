{
  "name": "website",
  "version": "1.0.0",
  "repository": "https://github.com/sean-krail/website",
  "author": "Sean Krail <sean@krail.dev>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "postinstall": "husky install",
    "prepare": "husky install",
    "lint": "eslint --fix src/ --ext .js,.jsx,.ts,.tsx",
    "lint-check": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write src/",
    "format-check": "prettier --check src/",
    "clean": "yarn rimraf dist/",
    "start": "yarn clean && parcel src/*.html",
    "build": "yarn clean && parcel build src/*.html"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@fontsource/lato": "^4.5.10",
    "@fontsource/raleway": "^4.5.12",
    "@fontsource/roboto-mono": "^4.5.10",
    "@parcel/compressor-brotli": "^2.8.3",
    "@parcel/compressor-gzip": "^2.8.3",
    "@parcel/config-default": "^2.8.3",
    "@parcel/core": "^2.8.3",
    "@parcel/transformer-image": "^2.8.3",
    "@types/node": "^14.18.47",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-types": "^6.26.0",
    "cssnano": "^6.0.1",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-parcel": "^1.10.6",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.3",
    "node-gyp": "^9.3.1",
    "normalize.css": "^8.0.1",
    "parcel": "^2.8.3",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sharp": "^0.31.1",
    "typescript": "^5.0.4"
  },
  "browserslist": "defaults",
  "packageManager": "yarn@3.5.1"
}
